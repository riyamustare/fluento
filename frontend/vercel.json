{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://fluento-backend.onrender.com/api",
    "VITE_FASTAPI_URL": "https://fluento-ai-api.onrender.com/api"
  }
}
